<!-- Animation for power lever quadrant -->

<animation>
	<type>rotate</type>
	<object-name>flaplever</object-name>
	<property>controls/flight/flaps[0]</property>
 		<interpolation>
			<entry><ind>0</ind><dep>0</dep></entry>
			<entry><ind>1</ind><dep>45</dep></entry>
		</interpolation>
	<center>
		<x-m>-8.520</x-m>
		<y-m>0.0</y-m>
		<z-m>-1.495</z-m>
	</center>
	<axis>
		<x>0</x>
		<y>1</y>
		<z>0</z>
	</axis>
</animation>	
	
<animation>
	<type>rotate</type>
	<object-name>PL1</object-name>
	<property>controls/engines/engine[2]/throttle</property>
 		<interpolation>
			<entry><ind>0</ind><dep>0</dep></entry>
			<entry><ind>1</ind><dep>65</dep></entry>
		</interpolation>
	<center>
		<x-m>-8.520</x-m>
		<y-m>0.0</y-m>
		<z-m>-1.495</z-m>
	</center>
	<axis>
		<x>0</x>
		<y>-1</y>
		<z>0</z>
	</axis>
</animation>

