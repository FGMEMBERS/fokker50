<?xml version="1.0"?>
<PropertyList>

	<path>nl.ac</path>

<!-- Lighting!  -->

 <animation>
  <type>material</type>
    <object-name>face</object-name> 
    <object-name>needle</object-name>
  <emission>
    <red>0.7</red>
    <green>0.7</green>
    <blue>0.7</blue>
	<factor-prop>controls/lighting/instruments-norm</factor-prop>
  </emission>
</animation>
 <animation>
  <type>material</type>
    <object-name>casing</object-name>
  <emission>
    <red>0.07</red>
    <green>.06</green>
    <blue>0.05</blue>
	<factor-prop>controls/lighting/dome-norm</factor-prop>
  </emission>
</animation>

	<animation>
		<type>rotate</type>
		<object-name>needle</object-name>
		<property>engines/engine/n1[0]</property>

		<interpolation>
			<entry><ind>0</ind><dep>0</dep></entry>
			<entry><ind>90</ind><dep>108</dep></entry>
			<entry><ind>100</ind><dep>200</dep></entry>
			<entry><ind>110</ind><dep>296</dep></entry>
		</interpolation>

		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
		</center>

		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>

	</animation>
	
	
</PropertyList>

