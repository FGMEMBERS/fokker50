<!-- Make the Alt-Sel knob work -->

<animation>
	<type>pick</type>
	<object-name>altsel</object-name>
	<visible>true</visible>
	<action>
		<button>0</button>
		<repeatable>false</repeatable>
		<binding>
			<command>property-adjust</command>
			<property>systems/fokker50/fmp/window-alt</property>
			<step>100</step>
			<min>0</min>
			<max>30000</max>
			<wrap>false</wrap>
		</binding>	
	</action>
	<action>
		<button>1</button>
		<repeatable>false</repeatable>
		<binding>
			<command>property-adjust</command>
			<property>systems/fokker50/fmp/window-alt</property>
			<step>-100</step>
			<min>0</min>
			<max>30000</max>
			<wrap>false</wrap>
		</binding>	
	</action>
	<action>
		<button>3</button>
		<repeatable>true</repeatable>
		<binding>
			<command>property-adjust</command>
			<property>systems/fokker50/fmp/window-alt</property>
			<step>1000</step>
			<min>0</min>
			<max>30000</max>
			<wrap>false</wrap>
		</binding>	
	</action>
	<action>
		<button>4</button>
		<repeatable>true</repeatable>
		<binding>
			<command>property-adjust</command>
			<property>systems/fokker50/fmp/window-alt</property>
