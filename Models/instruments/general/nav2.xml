<!-- Here's some pick animations for adjusting freqs: -->
<animation>
	<type>pick</type>
	<object-name>tuningknob</object-name>
	<visible>true</visible>
	<action>
		<button>0</button>
		<repeatable>false</repeatable>
		<binding>
			<command>property-adjust</command>
			<property>instrumentation/nav[1]/frequencies/standby-mhz</property>
			<step>0.05</step>
			<min>108.0</min>
			<max>117.95</max>
			<wrap>true</wrap>
		</binding>	
	</action>
	<action>
		<button>1</button>
		<repeatable>false</repeatable>
		<binding>
			<command>property-adjust</command>
			<property>instrumentation/nav[1]/frequencies/standby-mhz</property>
			<step>-0.05</step>
			<min>108.0</min>
			<max>117.95</max>
			<wrap>true</wrap>
		</binding>		
	</action>
	<action>
		<button>3</button>
		<repeatable>true</repeatable>
		<binding>
			<command>property-adjust</command>
			<property>instrumentation/nav[1]/frequencies/standby-mhz</property>
			<step>1</step>
			<min>108.0</min>
			<max>117.95</max>
			<wrap>true</wrap>
		</binding>	
	</action>
	<action>
		<button>4</button>
		<repeatable>true</repeatable>
		<binding>
			<command>property-adjust</command>
			<property>instrumentation/nav[1]/frequencies/standby-mhz</property>
			<step>-1</step>
			<min>108.0</min>
			<max>117.95</max>
			<wrap>true</wrap>
		</binding>	
	</action>
</animation>

<!-- Finally, press the left-hand knob to swap the freqs -->
<animation>
	<type>pick</type>
	<object-name>modeknob</object-name>
	<visible>true</visible>
	<action>
		<button>0</button>
		<repeatable>false</repeatable>
		<binding>
			<command>property-swap</command>
			<property>instrumentation/nav[1]/frequencies/standby-mhz</property>
			<property>instrumentation/nav[1]/frequencies/selected-mhz</property>
		</binding>	
	</action>
</animation>

	<!-- LoD -->
	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>30</max-m>
	</animation>

</PropertyList>

