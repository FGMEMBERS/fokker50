<?xml version="1.0"?>
<PropertyList>

	<path>nav.ac</path>

<!-- Lighting -->

 <animation>
  <type>material</type>
    <object-name>actdig1</object-name>
    <object-name>actdig2</object-name>
    <object-name>actdig3</object-name>
    <object-name>actdig01</object-name>
    <object-name>actdig02</object-name>
    <object-name>sbydig1</object-name>
    <object-name>sbydig2</object-name>
    <object-name>sbydig3</object-name>
    <object-name>sbydig01</object-name>
    <object-name>sbydig02</object-name>
  <emission>
    <red>0.9</red>
    <green>.9</green>
    <blue>0.9</blue>
	<factor-prop>systems/electrical/serviceable</factor-prop>
  </emission>
</animation>
 <animation>
  <type>material</type>
    <object-name>frame</object-name>
    <object-name>modeknob</object-name>
    <object-name>tuningknob</object-name>
  <emission>
    <red>0.07</red>
    <green>.06</green>
    <blue>0.05</blue>
	<factor-prop>controls/lighting/dome-norm</factor-prop>
  </emission>
</animation>


	<animation>
