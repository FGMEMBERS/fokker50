<?xml version="1.0"?>
 
<PropertyList>

 <fx>

  <engine0>
   <name>whine</name>
   <path>Sounds/whine.wav</path>
   <mode>looped</mode>
   <property>/engines/engine[0]/running</property>
   <volume>
     <factor>0.00025</factor>
   </volume> 
   <pitch>
    <property>/engines/engine[0]/n1</property>
    <type>lin</type>
    <factor>.4</factor>
   </pitch>
  </engine0>

  <engine0>
   <name>engine0</name>
   <mode>looped</mode>
   <path>Sounds/turboprop1.wav</path>
   <condition>
    <property>/engines/engine[0]/running</property>
   </condition>
<!--   <volume>
    <property>/engines/engine[0]/thruster/rpm</property>
    <type>abs</type>
    <factor>0.00069</factor>
    <offset>-0.328</offset>
    <min>0.65</min>
    <max>1.0</max>
   </volume>  -->
   <volume>
    <property>/engines/engine[0]/thrust_lb</property>
    <type>abs</type>
    <factor>0.00005</factor>
    <offset>-0</offset>
    <min>0.15</min>
    <max>0.35</max>
   </volume>
<!--   <volume>
    <property>/engines/engine[0]/nozzle-pos-norm</property>
    <factor>-0.7</factor>
    <offset>0.7</offset>
   </volume>  -->
   <pitch>
    <property>/engines/engine[0]/thruster/rpm</property>
    <factor>0.001</factor>
    <offset>0.9</offset>
    <min>0.7</min>
    <max>10</max>
   </pitch>
  </engine0>

  <engine1>
   <name>whine</name>
   <path>Sounds/whine.wav</path>
   <mode>looped</mode>
   <property>/engines/engine[1]/running</property>
   <volume>
     <factor>0.00025</factor>
   </volume>
   <pitch>
    <property>/engines/engine[1]/n1</property>
    <type>lin</type>
    <factor>.006</factor>
   </pitch>
  </engine1>

